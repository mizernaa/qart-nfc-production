{
  "buildCommand": "npm run build",
  "installCommand": "npm install --force",
  "env": {
    "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=8192",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "functions": {
    "app/api/**/*": {
      "maxDuration": 60
    }
  },
  "framework": "nextjs"
}